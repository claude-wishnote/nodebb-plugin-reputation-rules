{
  "name": "nodebb-plugin-score-rules",
  "version": "0.0.2",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claude-wishnote/nodebb-plugin-score-rules.git"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "score-rules",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/claude-wishnote/nodebb-plugin-score-rules/issues"
  },
  "nbbpm": {
    "compatibility": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-angular": "19.3.0",
    "eslint": "9.x",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.x",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  },
  "homepage": "https://github.com/claude-wishnote/nodebb-plugin-score-rules#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ppcloudercd"
}
